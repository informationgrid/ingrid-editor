{
  "name": "ige-ng",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "fix-prettier": "prettier -w src",
    "browserslist": "browserslist --update-db",
    "build": "ng build ige-ng",
    "prod": "ng build ige-ng --configuration production",
    "prod-stats": "ng build ige-ng --configuration production --stats-json",
    "update-angular": "ng update @angular/core --allow-dirty --force",
    "update-material": "ng update @angular/cdk-experimental @angular/material @angular/material-experimental --allow-dirty --force",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "wba": "webpack-bundle-analyzer dist/stats.json",
    "sme": "source-map-explorer",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.1.0",
    "@angular/cdk-experimental": "^15.1.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.1.0",
    "@angular/material-experimental": "^15.1.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@datorama/akita": "^7.1.1",
    "@flowjs/flow.js": "^2.14.1",
    "@flowjs/ngx-flow": "^0.6.0",
    "@geoman-io/leaflet-geoman-free": "^2.13.1",
    "@ngneat/dirty-check-forms": "^3.0.3",
    "@ngneat/transloco": "4.2.1",
    "@ngneat/until-destroy": "^9.2.2",
    "@ngx-formly/core": "6.1.1",
    "@ngx-formly/material": "6.1.1",
    "@stomp/rx-stomp": "^1.2.0",
    "angular-mixed-cdk-drag-drop": "^2.0.0",
    "angular-split": "^14.1.0",
    "angular2-csv": "^0.2.9",
    "cronstrue": "^2.21.0",
    "file-saver": "^2.0.5",
    "keycloak-angular": "^13.0.0",
    "keycloak-js": "^19.0.3",
    "leaflet": "^1.9.3",
    "material-components-web": "14.0.0",
    "material-design-icons-iconfont": "^6.7.0",
    "ngx-mat-select-search": "^7.0.0",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "tslib": "^2.3.0",
    "wicket": "^1.3.8",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angularclass/hmr": "^3.0.0",
    "@datorama/akita-ngdevtools": "^7.0.0",
    "@ngneat/spectator": "^13.0.0",
    "@types/flowjs": "~2.13.9",
    "@types/jasmine": "~3.6.7",
    "@types/leaflet": "^1.9.0",
    "@types/node": "^18.6.3",
    "akita-schematics": "^2.0.1",
    "husky": "^8.0.1",
    "jasmine-reporters": "^2.5.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.1",
    "source-map-explorer": "^2.5.3",
    "stop-only": "^3.1.2",
    "typescript": "~4.8.4"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.0",
    "node": ">= 14.0.0"
  },
  "lint-staged": {
    "*.{ts,scss,js,css,html,md,json}": "prettier --check",
    "*.{spec,cy}.ts": "yarn stop-only --folder"
  }
}
