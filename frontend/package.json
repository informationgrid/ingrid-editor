{
  "name": "ige-ng",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-hmr": "ng serve --hmr",
    "start-ie11": "ng serve --configuration es5",
    "hmr": "ng serve --configuration hmr",
    "build": "ng build ige-ng",
    "postinstallX": "grep -rl node_modules/@material/ -e '@import \"@material' | xargs sed -i \"\" 's/@material/~@material/g'",
    "prod": "ng build ige-ng --prod",
    "prod-stats": "ng build ige-ng --prod --stats-json",
    "update": "ng update @angular/cli @angular/core @angular/material @angular/material-experimental @datorama/akita --allow-dirty --force",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "wba": "webpack-bundle-analyzer dist/stats.json",
    "sme": "source-map-explorer",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.4",
    "@angular/cdk": "^12.0.4",
    "@angular/cdk-experimental": "^11.2.5",
    "@angular/common": "^12.0.4",
    "@angular/compiler": "^12.0.4",
    "@angular/core": "^12.0.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^12.0.4",
    "@angular/material": "^12.0.4",
    "@angular/material-experimental": "^12.0.4",
    "@angular/platform-browser": "^12.0.4",
    "@angular/platform-browser-dynamic": "^12.0.4",
    "@angular/router": "^12.0.4",
    "@datorama/akita": "^6.2.0",
    "@ngneat/dirty-check-forms": "^1.1.0",
    "@ngneat/until-destroy": "^8.0.4",
    "@ngx-formly/core": "5.10.21",
    "@ngx-formly/material": "5.10.21",
    "@stomp/ng2-stompjs": "^8.0.0",
    "angular-split": "^5.0.0",
    "classlist.js": "^1.1.20150312",
    "cronstrue": "^1.114.0",
    "leaflet": "^1.7.1",
    "mat-table-exporter": "^10.2.3",
    "material-components-web": "11.0.0",
    "material-design-icons-iconfont": "^6.1.0",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "smoothscroll-polyfill": "^0.4.4",
    "tslib": "^2.3.0",
    "web-animations-js": "^2.3.2",
    "wicket": "^1.3.6",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.4",
    "@angular/cli": "^12.0.4",
    "@angular/compiler-cli": "^12.0.4",
    "@angularclass/hmr": "^3.0.0",
    "@datorama/akita-ngdevtools": "^6.0.0",
    "@ngneat/spectator": "^7.2.0",
    "@types/jasmine": "~3.6.7",
    "@types/leaflet": "^1.7.2",
    "@types/leaflet-areaselect": "^0.1.4",
    "@types/node": "^12.11.1",
    "akita-schematics": "^2.0.1",
    "codelyzer": "^6.0.2",
    "husky": "^6.0.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.1",
    "source-map-explorer": "^2.5.2",
    "typescript": "~4.2.4"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.0",
    "node": ">= 10.0.0"
  },
  "lint-staged": {
    "*.{ts,scss,js,css,html,md}": "prettier --check"
  }
}
