{
  "name": "ige-ng",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-hmr": "ng serve --hmr",
    "start-ie11": "ng serve --configuration es5",
    "hmr": "ng serve --configuration hmr",
    "build": "ng build ige-ng",
    "postinstallX": "grep -rl node_modules/@material/ -e '@import \"@material' | xargs sed -i \"\" 's/@material/~@material/g'",
    "prod": "ng build ige-ng --configuration production",
    "prod-stats": "ng build ige-ng --configuration production --stats-json",
    "update": "ng update @angular/cli @angular/core @angular/cdk-experimental @angular/material @angular/material-experimental @angular/flex-layout @datorama/akita --allow-dirty --force",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "wba": "webpack-bundle-analyzer dist/stats.json",
    "sme": "source-map-explorer",
    "prepare": "cd .. && husky install frontend/.husky",
    "stop-only": "stop-only"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.8",
    "@angular/cdk": "^13.3.7",
    "@angular/cdk-experimental": "^13.3.7",
    "@angular/common": "^13.3.8",
    "@angular/compiler": "^13.3.8",
    "@angular/core": "^13.3.8",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^13.3.8",
    "@angular/material": "^13.3.7",
    "@angular/material-experimental": "^13.3.7",
    "@angular/platform-browser": "^13.3.8",
    "@angular/platform-browser-dynamic": "^13.3.8",
    "@angular/router": "^13.3.8",
    "@datorama/akita": "^7.1.1",
    "@flowjs/flow.js": "^2.14.1",
    "@flowjs/ngx-flow": "^0.4.6",
    "@ngneat/dirty-check-forms": "^1.1.0",
    "@ngneat/transloco": "3.1.3",
    "@ngneat/until-destroy": "^9.0.0",
    "@ngx-formly/core": "5.12.1",
    "@ngx-formly/material": "5.12.1",
    "@stomp/ng2-stompjs": "^8.0.0",
    "angular-split": "^13.1.0",
    "cronstrue": "^1.123.0",
    "keycloak-angular": "^9.1.0",
    "keycloak-js": "^16.1.1",
    "leaflet": "^1.7.1",
    "mat-table-exporter": "^10.2.4",
    "material-components-web": "14.0.0",
    "material-design-icons-iconfont": "^6.7.0",
    "ngx-mat-select-search": "^4.1.1",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "tslib": "^2.3.0",
    "wicket": "^1.3.8",
    "zone.js": "~0.11.4",
    "file-saver": "^2.0.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular/cli": "^13.3.5",
    "@angular/compiler-cli": "^13.3.8",
    "@angularclass/hmr": "^3.0.0",
    "@datorama/akita-ngdevtools": "^7.0.0",
    "@ngneat/spectator": "^10.0.0",
    "@types/jasmine": "~3.6.7",
    "@types/leaflet": "^1.7.2",
    "@types/leaflet-areaselect": "^0.1.4",
    "@types/node": "^12.11.1",
    "akita-schematics": "^2.0.1",
    "husky": "^7.0.4",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "lint-staged": "^12.3.2",
    "prettier": "2.5.1",
    "source-map-explorer": "^2.5.2",
    "stop-only": "^3.1.2",
    "typescript": "~4.5.5"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.0",
    "node": ">= 10.0.0"
  },
  "lint-staged": {
    "*.{ts,scss,js,css,html,md}": "prettier --check",
    "*.spec.ts": "yarn stop-only --folder"
  }
}
