{
  "name": "ige-ng",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-hmr": "ng serve --hmr=true",
    "start-ie11": "ng serve --configuration es5",
    "hmr": "ng serve --configuration hmr",
    "build": "ng build ige-ng",
    "postinstallX": "grep -rl node_modules/@material/ -e '@import \"@material' | xargs sed -i \"\" 's/@material/~@material/g'",
    "prod": "ng build ige-ng --prod",
    "prod-stats": "ng build ige-ng --prod --stats-json",
    "update": "ng update @angular/cli @angular/core @angular/material @angular/material-experimental @datorama/akita --allow-dirty --force",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "wba": "webpack-bundle-analyzer dist/stats.json",
    "sme": "source-map-explorer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.0",
    "@angular/cdk": "^11.1.0",
    "@angular/cdk-experimental": "^11.1.0",
    "@angular/common": "^11.1.0",
    "@angular/compiler": "^11.1.0",
    "@angular/core": "^11.1.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.1.0",
    "@angular/material": "^11.1.0",
    "@angular/material-experimental": "^11.1.0",
    "@angular/platform-browser": "^11.1.0",
    "@angular/platform-browser-dynamic": "^11.1.0",
    "@angular/router": "^11.1.0",
    "@datorama/akita": "^5.3.0",
    "@ngneat/dirty-check-forms": "^1.0.1",
    "@ngneat/forms-manager": "^2.3.0",
    "@ngneat/until-destroy": "^8.0.3",
    "@ngx-formly/core": "5.10.11",
    "@ngx-formly/material": "5.10.11",
    "angular-split": "^5.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.3",
    "leaflet": "^1.7.1",
    "material-components-web": "9.0.0",
    "material-design-icons-iconfont": "^6.1.0",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "smoothscroll-polyfill": "^0.4.4",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "wicket": "^1.3.6",
    "xmlbuilder": "^10.1.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "^11.1.0",
    "@angular/compiler-cli": "^11.1.0",
    "@angularclass/hmr": "^3.0.0",
    "@datorama/akita-ngdevtools": "^5.0.3",
    "@ngneat/spectator": "^6.1.3",
    "@types/jasmine": "~3.6.3",
    "@types/leaflet": "^1.5.21",
    "@types/leaflet-areaselect": "^0.1.4",
    "@types/node": "^12.11.1",
    "akita-schematics": "^2.0.1",
    "codelyzer": "^6.0.1",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "ng-packagr": "^11.0.2",
    "raw-loader": "3.1.0",
    "source-map-explorer": "^2.4.2",
    "ts-node": "~7.0.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.0",
    "node": ">= 10.0.0"
  }
}
