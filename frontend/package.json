{
  "name": "ige-ng",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "fix-prettier": "prettier -w src",
    "browserslist": "browserslist --update-db",
    "build": "ng build ige-ng",
    "prod": "ng build ige-ng --configuration production",
    "prod-stats": "ng build ige-ng --configuration production --stats-json",
    "update-angular": "ng update @angular/cli --allow-dirty --force",
    "update-material": "ng update @angular/cdk @angular/material @angular/material-experimental --allow-dirty --force",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "wba": "webpack-bundle-analyzer dist/stats.json",
    "sme": "source-map-explorer",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.2",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.0.2",
    "@angular/compiler": "^17.0.2",
    "@angular/core": "^17.0.2",
    "@angular/forms": "^17.0.2",
    "@angular/material": "^17.0.0",
    "@angular/platform-browser": "^17.0.2",
    "@angular/platform-browser-dynamic": "^17.0.2",
    "@angular/router": "^17.0.2",
    "@datorama/akita": "^8.0.1",
    "@flowjs/flow.js": "^2.14.1",
    "@flowjs/ngx-flow": "^0.6.0",
    "@geoman-io/leaflet-geoman-free": "^2.14.2",
    "@ngneat/dirty-check-forms": "^3.0.3",
    "@ngneat/transloco": "6.0.0",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngx-formly/core": "6.2.2",
    "@ngx-formly/material": "6.2.2",
    "@stomp/rx-stomp": "^2.0.0",
    "@stomp/stompjs": "^7.0.0",
    "angular-mixed-cdk-drag-drop": "^2.1.0",
    "angular-split": "^16.2.1",
    "cronstrue": "^2.43.0",
    "diff": "^5.1.0",
    "file-saver-es": "^2.0.5",
    "keycloak-angular": "^15.0.0",
    "keycloak-js": "^22.0.4",
    "leaflet": "^1.9.4",
    "material-design-icons-iconfont": "^6.7.0",
    "ngx-mat-select-search": "^7.0.5",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "wicket": "^1.3.8",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.2",
    "@datorama/akita-ngdevtools": "^7.0.0",
    "@ngneat/spectator": "^15.0.1",
    "@types/file-saver-es": "~2.0.3",
    "@types/flowjs": "~2.13.12",
    "@types/jasmine": "~5.1.2",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.9.0",
    "akita-schematics": "^2.0.1",
    "husky": "^8.0.3",
    "jasmine-core": "^5.1.1",
    "jasmine-reporters": "^2.5.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-sourcemap-loader": "^0.4.0",
    "lint-staged": "^15.1.0",
    "prettier": "3.0.3",
    "source-map-explorer": "^2.5.3",
    "stop-only": "^3.3.1",
    "typescript": "~5.2.2"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 3.0.0",
    "node": ">= 18.13.0"
  },
  "lint-staged": {
    "*.{ts,scss,js,css,html,md,json}": "prettier --check",
    "*.{spec,cy}.ts": "yarn stop-only --folder"
  },
  "packageManager": "yarn@4.0.2"
}
