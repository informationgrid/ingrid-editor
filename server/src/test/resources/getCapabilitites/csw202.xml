                <ows:Value>DCP</ows:Value>
                <ows:Value>CouplingType</ows:Value>
            </ows:Constraint>
            <ows:Constraint name="AdditionalQueryables">
                <ows:Value>Fees</ows:Value>
            </ows:Constraint>
        </ows:Operation>
