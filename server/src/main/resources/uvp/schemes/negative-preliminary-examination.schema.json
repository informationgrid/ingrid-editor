{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/NegativePreliminaryExamination",
  "definitions": {
    "NegativePreliminaryExamination": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "_uuid": {
          "type": "string",
          "format": "uuid"
        },
        "_type": {
          "const": "UvpNegativePreliminaryExaminationDoc"
        },
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "publisher": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Publisher"
          },
          "minItems": 1,
          "maxItems": 1
        },
        "spatial": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Spatial"
          },
          "minItems": 1
        },
        "decisionDate": {
          "type": "string",
          "format": "date-time"
        },
        "eiaNumber": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/KeyValue"
          },
          "minItems": 1
        },
        "uvpNegativeDecisionDocs": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Doc"
          },
          "minItems": 1
        }
      },
      "required": [
        "_uuid",
        "_type",
        "title",
        "description",
        "publisher",
        "spatial",
        "decisionDate",
        "eiaNumber",
        "uvpNegativeDecisionDocs"
      ],
      "title": "NegativePreliminaryExamination"
    },
    "KeyValue": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "key": {
          "type": [
            "string",
            "null"
          ]
        },
        "value": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "title": "KeyValue"
    },
    "Doc": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "title": {
          "type": "string"
        },
        "downloadURL": {
          "$ref": "#/definitions/DownloadURL"
        }
      },
      "required": [],
      "title": "Doc"
    },
    "DownloadURL": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "asLink": {
          "type": "boolean"
        },
        "value": {
          "type": "string"
        },
        "uri": {
          "type": "string"
        }
      },
      "required": [],
      "title": "DownloadURL"
    },
    "Date": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "start": {
          "type": "string",
          "format": "date-time"
        },
        "end": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [],
      "title": "Date"
    },
    "Publisher": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "ref": {
          "type": "string"
        },
        "type": {
          "$ref": "#/definitions/KeyValue"
        }
      },
      "required": [
        "ref",
        "type"
      ],
      "title": "Publisher"
    },
    "Spatial": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "value": {
          "$ref": "#/definitions/BoundingBox"
        }
      },
      "required": [],
      "title": "Spatial"
    },
    "BoundingBox": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "lat1": {
          "type": "number"
        },
        "lat2": {
          "type": "number"
        },
        "lon1": {
          "type": "number"
        },
        "lon2": {
          "type": "number"
        }
      },
      "required": [
        "lat1",
        "lat2",
        "lon1",
        "lon2"
      ],
      "title": "Value"
    }
  }
}
