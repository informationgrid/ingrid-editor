{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "NegativePreliminaryExamination#1.0.0",
  "$ref": "#/definitions/NegativePreliminaryExamination",
  "definitions": {
    "NegativePreliminaryExamination": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "_uuid": {
          "type": "string",
          "format": "uuid"
        },
        "_type": {
          "const": "UvpNegativePreliminaryExaminationDoc"
        },
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "pointOfContact": {
          "type": "array",
          "items": {
            "$ref": "refs/general.schema.json#/definitions/PointOfContact"
          },
          "minItems": 1,
          "maxItems": 1
        },
        "spatial": {
          "type": "array",
          "items": {
            "$ref": "refs/general.schema.json#/definitions/Spatial"
          },
          "minItems": 1
        },
        "decisionDate": {
          "type": "string",
          "format": "date-time"
        },
        "eiaNumbers": {
          "type": "array",
          "items": {
            "$ref": "refs/general.schema.json#/definitions/KeyValue"
          },
          "minItems": 1
        },
        "uvpNegativeDecisionDocs": {
          "type": "array",
          "items": {
            "$ref": "refs/general.schema.json#/definitions/Doc"
          },
          "minItems": 1
        }
      },
      "required": [
        "_uuid",
        "_type",
        "title",
        "description",
        "pointOfContact",
        "spatial",
        "decisionDate",
        "eiaNumbers",
