{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/SpatialOrLine",
  "definitions": {
    "SpatialOrLine": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "_uuid": {
          "type": "string",
          "format": "uuid"
        },
        "_type": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "publisher": {
          "type": "array",
          "items": {
            "$ref": "refs/general.schema.json#/definitions/Publisher"
          },
          "minItems": 1,
          "maxItems": 1
        },
        "spatial": {
          "type": "array",
          "items": {
            "$ref": "refs/general.schema.json#/definitions/Spatial"
          },
          "minItems": 1
        },
        "receiptDate": {
          "type": "string",
          "format": "date-time"
        },
        "eiaNumbers": {
          "type": "array",
          "items": {
            "$ref": "refs/general.schema.json#/definitions/KeyValue"
          },
          "minItems": 1
        },
        "processingSteps": {
          "type": "array",
          "items": {
            "$ref": "refs/processing-steps-default.schema.json"
          }
        }
      },
      "required": [
        "_uuid",
        "_type",
        "title",
        "description",
        "publisher",
        "spatial",
        "receiptDate",
        "eiaNumbers"
      ],
      "title": "SpatialOrLine"
    }
  }
}
