{
  "name": "ige-remastered-cli",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config src/environments/proxy.config.json",
    "startHmr": "ng serve --hmr -e=hmr --proxy-config src/environments/proxy.config.json",
    "startIp": "ng serve --host 192.168.0.132",
    "startAot": "ng serve --aot",
    "build": "ng build",
    "profiles": "webpack --config src/profiles/webpack.config.js",
    "prod": "ng build --prod --stats-json --sourcemaps",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "wba": "webpack-bundle-analyzer dist/stats.json",
    "sme": "source-map-explorer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.4",
    "@angular/common": "^5.2.4",
    "@angular/compiler": "^5.2.4",
    "@angular/core": "^5.2.4",
    "@angular/forms": "^5.2.4",
    "@angular/http": "^5.2.4",
    "@angular/platform-browser": "^5.2.4",
    "@angular/platform-browser-dynamic": "^5.2.4",
    "@angular/router": "^5.2.4",
    "angular-split": "^1.0.0-rc.3",
    "bootstrap": "4.0.0-beta.2",
    "chart.js": "^2.7.1",
    "core-js": "^2.5.3",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "leaflet": "^1.3.1",
    "ngx-bootstrap": "^2.0.2",
    "primeng": "^5.2.0",
    "rxjs": "^5.5.6",
    "scriptjs": "^2.5.8",
    "web-animations-js": "^2.3.1",
    "xmlbuilder": "^9.0.7",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.8",
    "@angular/compiler-cli": "^5.2.4",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "2.5.53",
    "@types/leaflet": "^1.2.5",
    "@types/leaflet-areaselect": "^0.1.3",
    "@types/node": "^7.0.54",
    "codelyzer": "~3.1.1",
    "jasmine-core": "~2.6.4",
    "jasmine-spec-reporter": "~4.1.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "^1.1.1",
